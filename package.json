{
  "name": "larsroettig-next",
  "description": "Personal Magento Blog of Lars Roettig",
  "version": "1.0.0",
  "private": true,
  "author": "Lars Roettig <l.roettig@techdivision.com",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "typescript",
    "Magento"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "build:rss": "ts-node src/scripts/generate-rss.ts",
    "postbuild": "next-sitemap && yarn build:rss",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "e2e": "start-test dev 3000 cy:open",
    "e2e:ci": "cypress run",
    "cy:open": "cypress open",
    "pree2e": "rm -rf .nyc_output coverage .next || true",
    "check-coverage-limits": "nyc report --reporter=text-summary --check-coverage --statements 100"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@bradgarropy/next-link": "1.0.2",
    "@next/bundle-analyzer": "10.1.1",
    "@plaiceholder/next": "2.1.0",
    "@prefresh/next": "^1.4.6",
    "@tailwindcss/typography": "0.4.0",
    "cross-env": "7.0.3",
    "cypress-expect": "2.2.1",
    "date-fns": "2.19.0",
    "fast-glob": "3.2.5",
    "feed": "4.2.2",
    "gray-matter": "4.0.2",
    "next": "11.0.1",
    "next-compose-plugins": "2.2.1",
    "next-mdx-remote": "2.1.3",
    "next-pwa": "5.0.6",
    "next-seo": "4.23.0",
    "node-cache": "5.1.2",
    "plaiceholder": "2.0.1",
    "preact": "10.5.13",
    "prism-react-renderer": "1.2.0",
    "prismjs": "1.23.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "4.2.0",
    "react-protected-mailto": "1.0.3",
    "react-scroll-up-button": "1.6.4",
    "react-share": "4.4.0",
    "react-use-clipboard": "1.0.7",
    "reading-time": "1.3.0",
    "remark-autolink-headings": "6.0.1",
    "remark-copy-linked-files": "1.2.0",
    "remark-images": "2.0.0",
    "remark-slug": "6.0.0",
    "responsive-loader": "2.0.0",
    "sharp": "0.27.1"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@testing-library/cypress": "7.0.4",
    "@testing-library/react": "11.2.5",
    "@types/node": "14.14.37",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "autoprefixer": "10.2.6",
    "commitizen": "4.2.3",
    "cypress": "6.8.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.23.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "next-sitemap": "1.6.9",
    "postcss": "8.3.5",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.2.1",
    "start-server-and-test": "1.12.1",
    "tailwindcss": "2.2.4",
    "ts-node": "9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.28.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
